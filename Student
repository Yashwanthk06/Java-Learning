import java.util.*;
public class Student{
    public static void main (String[] args)
    {
    Scanner sc = new Scanner(System.in);
    int a[]=new  int[10];
    System.out.print("Enter the numbers to be added");
    for(int i =0; i<10; i++){
     a[i] = sc.nextInt();
    }---=-
    int c=0;
    int d;
    for(int i =0;i<10;i++){
        c=c+a[i];
    }
    d=c/10;
    System.out.println("The average is"+d);
}}
